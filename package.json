{
  "name": "my-app-name",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "strapi build && npm run copy:files",
    "copy:files": "mkdir -p build && cp -r data build/ && cp -r config build/ 2>/dev/null || true",
    "deploy": "strapi deploy",
    "develop": "strapi develop",
    "dev:local": "cp .env.local .env && strapi develop",
    "dev:prod": "cp .env.production .env && NODE_ENV=production strapi start",
    "start:local": "cp .env.local .env && strapi start",
    "start:prod": "cp .env.production .env && NODE_ENV=production strapi start",
    "seed:example": "node ./scripts/seed.js",
    "start": "strapi start",
    "strapi": "strapi"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "5.15.0",
    "@strapi/plugin-users-permissions": "5.15.0",
    "@strapi/strapi": "5.15.0",
    "better-sqlite3": "11.3.0",
    "fs-extra": "^10.0.0",
    "mime-types": "^2.1.27",
    "pg": "^8.16.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "strapi-provider-upload-supabase": "^1.0.0",
    "styled-components": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  }
}
